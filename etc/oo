								if(tipoAcc4.equals("Cloak")){
									if(posesionAcc4.equals("Normal")){
									
									accesorie4=new Cloak(acc4,descripcionAcc4,false,false,null);
									}
									
									if(posesionAcc4.equals("Posesión")){
										ArrayList<String> pos=new ArrayList<String>();
										pos.add(prop1Acc4);
										pos.add(prop2Acc4);
